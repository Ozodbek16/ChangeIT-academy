<div class="crud_main">
    <div class="crud">
        {{#each techs}}
        <div class="crud_item">
            <img src="{{uploadPath}}{{img}}" alt="img not found">
            <h2>{{title}}</h2>
            <a href="/admin/crud/techs/delete/{{_id}}">Delete</a>
        </div>
        {{/each}}
    </div>

    <form class="crud_form" action="/admin/crud/techs/addNew" method="post" enctype="multipart/form-data">
        <h2>Add new</h2>
        <label for="file">Image</label>
        <input type="file" name="img" id="file" required>
        <br>

        <label for="title">Title</label>
        <input type="text" name="title" id="title" required>
        <br>
        <button type="submit">Send</button>
    </form>
</div>